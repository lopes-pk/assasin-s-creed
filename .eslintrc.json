{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-filename-extension": [
            "error",
            {
                "extensions": [".js", ".jsx"]
            }
        ],
        "global-require":"off",
        "import/prefer-default-export": "off",
        "import/no-unresolved": "off",
        "no-unused-expressions": ["error", {"allowTaggedTemplates":true}]
    }
}
